{
  "name": "OQL",
  "scopeName": "source.oql",
  "patterns": [
    {
      "include": "#string"
    }
  ],
  "repository": {
    "string": {
      "begin": "(?<!\\\\)(\")",
      "beginCaptures": {
        "1": {
          "name": "string.begin.oql"
        }
      },
      "end": "(\")",
      "endCaptures": {
        "1": {
          "name": "string.end.oql"
        }
      },
      "name": "string.quoted.double.oql",
      "patterns": [
        {
          "match": "\\\\.",
          "name": "constant.character.escape.oql"
        }
      ]
    }
  }
}